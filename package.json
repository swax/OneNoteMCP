  "devDependencies": {
    "@types/jest": "29.5.14",
    "@types/node": "24.0.1",
    "@typescript-eslint/eslint-plugin": "8.34.0",
    "@typescript-eslint/parser": "8.34.0",
    "eslint": "9.29.0",
    "jest": "30.0.0",
    "prettier": "3.5.3",
    "ts-jest": "29.4.0",
    "typescript": "5.8.3"
  },
  "keywords": [
    "mcp",
