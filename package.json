{
  "name": "mcp-server-onenote",
  "version": "0.1.0",
  "description": "OneNote MCP Server Implementation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rmdir /s dist",
    "build": "tsc",
    "test": "jest --coverage",
    "run": "node dist/index.js",
